{
  "name": "tekblogg-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn partytown && next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "partytown": "partytown copylib public/~partytown",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
