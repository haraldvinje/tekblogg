    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-bundle-analyzer": "^0.6.4",
    "next-sitemap": "^4.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.0",
