    "prettier:fix": "prettier --write src",
    "format": "yarn lint:fix && yarn prettier:fix",
    "ts-lint": "tsc --noEmit --incremental",
    "clean": "rm -rf node_modules .next src/.next",
    "clean:install": "rm -rf node_modules .next && yarn",
    "prepare": "cd .. && husky install tekblogg-web/.husky"
  },
